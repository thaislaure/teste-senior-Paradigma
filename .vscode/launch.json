{
    "version": "0.2.0",
    "configurations": [
        { 
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build", 
            "program": "${workspaceFolder}/SampleTree/bin/Debug/netcoreapp3.1/SampleTree.dll",
            "args": [],
            "cwd": "${workspaceFolder}/SampleTree", 
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}